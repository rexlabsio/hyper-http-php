{
  "name": "rexlabs/hyper-http",
  "description": "HTTP client for PHP",
  "keywords": [
    "http",
    "library",
    "restful",
    "api",
    "json",
    "fluent",
    "request",
    "post"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Jodie Dunlop",
      "email": "jodie.dunlop@rexlabs.com.au"
    }
  ],
  "require": {
    "php": ">=7.0",
    "psr/log": "^1.0",
    "rexlabs/utility-belt": "^3.0",
    "guzzlehttp/guzzle": "^6.3",
    "rexlabs/array-object": "^2.0.2",
    "rtheunissen/guzzle-log-middleware": "^0.4.1",
    "namshi/cuzzle": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5.7"
  },
  "autoload": {
    "psr-4": {
      "Rexlabs\\HyperHttp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rexlabs\\HyperHttp\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "tests": "phpunit --colors=always",
    "coverage": "phpunit --coverage-html ./tests/report"
  },
  "license": "MIT"
}
